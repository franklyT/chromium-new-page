{
  "content_scripts": [
    {
      "matches": ["http://*/*", "https://*/*"],
      "js": ["src/global.js"]
    }
  ],

  "manifest_version": 2,
  "name": "Tabby Tab",
  "key": "",
  "version": "0.0.1",
  "browser_action": { "default_icon": "favicon16.png" },
  "content_security_policy": "script-src 'self' https://*.google.com; object-src 'self'",
  "chrome_url_overrides": {
    "newtab": "index.html"
  },

  "description": "Lightweight tab modifier.",
  "oauth2": {
    "client_id": "261472421153-kupgm10l9p78lc51nceliq9iftql3t21.apps.googleusercontent.com",
    "scopes": [
      "https://www.googleapis.com/auth/calendar",
      "https://www.googleapis.com/auth/calendar.events"
    ]
  },

  "icons": {
    "16": "favicon16.png",
    "128": "favicon128.png"
  },

  "permissions": [
    "identity",
    "history",
    "storage",
    "bookmarks",
    "topSites"
  ]
}
