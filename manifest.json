{
  "content_scripts": [{
    "matches": ["http://*/*", "https://*/*"],
    "js": ["src/my.js"]
  }],

    "manifest_version": 2,
    "name": "My App",
    "version": "0.0.1",
    "browser_action": {"default_icon": "favicon16.png"},
    "content_security_policy": "script-src 'self' https://*.google.com; object-src 'self'",
    "chrome_url_overrides" : {
        "newtab": "index.html"
      },
    
    "description": "A plain text description",

    "icons": {
      "16": "favicon16.png",
      "128": "favicon128.png"
    },
    "optional_permissions": ["bookmarks", "topSites", "chrome://favicon/", "tabs"]

  }