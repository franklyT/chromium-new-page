{
  "content_scripts": [{
    "matches": ["http://*/*", "https://*/*"],
    "js": ["src/global-core.js"]
  }],
 

    "manifest_version": 2,
    "name": "Tabby Tab",
    "version": "0.0.1",
    "browser_action": {"default_icon": "favicon16.png"},
    "content_security_policy": "script-src 'self' https://*.google.com; object-src 'self'",
    "chrome_url_overrides" : {
        "newtab": "index.html"
      },
   
    "description": "Lightweight tab modifier.",

    "icons": {
      "16": "favicon16.png",
      "128": "favicon128.png"
    },

    "permissions":  ["storage", "activeTab", "bookmarks", "topSites", "chrome://favicon/"]
  }